{
  "name": "vite-f7-vue3",
  "version": "0.0.1",
  "license": "MIT",
  "author": "jiesir",
  "description": "移动端 Vue3 工程",
  "keywords": [
    "vue3",
    "framework7",
    "vite"
  ],
  "scripts": {
